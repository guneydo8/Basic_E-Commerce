
@{
    Layout = null;
}



<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
  
    <!-- Tell the browser to be responsive to screen width -->
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!-- Font Awesome -->
    <link rel="stylesheet" href="/Klasörler/web4/plugins/fontawesome-free/css/all.min.css">
    <!-- Ionicons -->
    <link rel="stylesheet" href="https://code.ionicframework.com/ionicons/2.0.1/css/ionicons.min.css">
    <!-- Tempusdominus Bbootstrap 4 -->
    <link rel="stylesheet" href="/Klasörler/web4/plugins/tempusdominus-bootstrap-4/css/tempusdominus-bootstrap-4.min.css">
    <!-- iCheck -->
    <link rel="stylesheet" href="/Klasörler/web4/plugins/icheck-bootstrap/icheck-bootstrap.min.css">
    <!-- JQVMap -->
    <link rel="stylesheet" href="/Klasörler/web4/plugins/jqvmap/jqvmap.min.css">
    <!-- Theme style -->
    <link rel="stylesheet" href="/Klasörler/web4/dist/css/adminlte.min.css">
    <!-- overlayScrollbars -->
    <link rel="stylesheet" href="/Klasörler/web4/plugins/overlayScrollbars/css/OverlayScrollbars.min.css">
    <!-- Daterange picker -->
    <link rel="stylesheet" href="/Klasörler/web4/plugins/daterangepicker/daterangepicker.css">
    <!-- summernote -->
    <link rel="stylesheet" href="/Klasörler/web4/plugins/summernote/summernote-bs4.css">
    <!-- Google Font: Source Sans Pro -->
    <link href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,400i,700" rel="stylesheet">
</head>
<body class="hold-transition sidebar-mini layout-fixed">
    <div class="wrapper">

        <!-- Navbar -->
        <nav class="main-header navbar navbar-expand navbar-white navbar-light">
            <!-- Left navbar links -->
            <ul class="navbar-nav">
                <li class="nav-item">
                    <a class="nav-link" data-widget="pushmenu" href="#" role="button"><i class="fas fa-bars"></i></a>
                </li>
                <li class="nav-item d-none d-sm-inline-block">
                    <a href="index3.html" class="nav-link">Home</a>
                </li>
                <li class="nav-item d-none d-sm-inline-block">
                    <a href="#" class="nav-link">Contact</a>
                </li>
            </ul>

            <!-- SEARCH FORM -->
            <form class="form-inline ml-3">
                <div class="input-group input-group-sm">
                    <input class="form-control form-control-navbar" type="search" placeholder="Search" aria-label="Search">
                    <div class="input-group-append">
                        <button class="btn btn-navbar" type="submit">
                            <i class="fas fa-search"></i>
                        </button>
                    </div>
                </div>
            </form>

            <!-- Right navbar links -->
            <ul class="navbar-nav ml-auto">
                <!-- Messages Dropdown Menu -->
              @Html.Action("Bildirim", "AdminLogin")
                <li class="nav-item">
                    <a class="nav-link" data-widget="control-sidebar" data-slide="true" href="#" role="button">
                        <i class="fas fa-th-large"></i>
                    </a>
                </li>
            </ul>
        </nav>
        <!-- /.navbar -->
        <!-- Main Sidebar Container -->
        <aside class="main-sidebar sidebar-dark-primary elevation-4">
            <!-- Brand Logo -->
            <a href="index3.html" class="brand-link">
                <img src="/Klasörler/web4/dist/img/AdminLTELogo.png" alt="AdminLTE Logo" class="brand-image img-circle elevation-3"
                     style="opacity: .8">
                <span class="brand-text font-weight-light">E-Ticaret Sistemi</span>
            </a>

            <!-- Sidebar -->
            <div class="sidebar">
              
                @Html.Action("Admin", "AdminLogin")

              
                <nav class="mt-2">
                    <ul class="nav nav-pills nav-sidebar flex-column" data-widget="treeview" role="menu" data-accordion="false">
                        <!-- Add icons to the links using the .nav-icon class
    with font-awesome or any other icon font library -->

                        <li class="nav-item">
                            <a href="/AnaSayfa/Index/" class="nav-link">
                                <i class="fas fa-home"></i>
                                <p>
                                    Ana Sayfa
                                    <span class="right badge badge-danger"></span>
                                </p>
                            </a>
                        </li>
                        <li class="nav-item">
                            <a href="/Kategoriler/Liste/" class="nav-link">
                                <i class="fa fa-bars"></i>
                                <p>
                                    Kategoriler
                                    <span class="right badge badge-danger"></span>
                                </p>
                            </a>
                        </li>
                        <li class="nav-item">
                            <a href="/Ürünler/Liste/" class="nav-link">
                                <i class="fas fa-shopping-cart"></i>
                                <p>
                                    Ürünler
                                    <span class="right badge badge-danger"></span>
                                </p>
                            </a>
                        </li>
                        <li class="nav-item">
                            <a href="/Departmanlar/Liste/" class="nav-link">
                                <i class="fa fa-address-card"></i>
                                <p>
                                    Departmanlar
                                    <span class="right badge badge-danger"></span>
                                </p>
                            </a>
                        </li>
                        <li class="nav-item">
                            <a href="/Müşteriler/Liste/" class="nav-link">
                                <i class="fa fa-users"></i>
                                <p>
                                    Müşteriler
                                    <span class="right badge badge-danger"></span>
                                </p>
                            </a>
                        </li>
                        <li class="nav-item">
                            <a href="/Personeller/Liste/" class="nav-link">
                                <i class="fa fa-user-circle"></i>
                                <p>
                                    Personeller
                                    <span class="right badge badge-danger"></span>
                                </p>
                            </a>
                        </li>

                        <li class="nav-item">
                            <a href="/İstatistikler/Liste/" class="nav-link">
                                <i class="fas fa-tachometer-alt"></i>
                                <p>
                                    İstatistikler
                                    <span class="right badge badge-danger"></span>
                                </p>
                            </a>
                        </li>
                        <li class="nav-item">
                            <a href="/Galeri/Liste/" class="nav-link">
                                <i class="fas fa-image"></i>
                                <p>
                                    Galeri
                                    <span class="right badge badge-danger"></span>
                                </p>
                            </a>
                        </li>
                        <li class="nav-item">
                            <a href="/Satışlar/Liste/" class="nav-link">
                                <i class="fa fa-credit-card"></i>
                                <p>
                                    Satışlar
                                    <span class="right badge badge-danger"></span>
                                </p>
                            </a>
                        </li>
                        <li class="nav-item">
                            <a href="/Kargo/Index/" class="nav-link">
                                <i class="fa fa-truck"></i>
                                <p>
                                    Kargolar
                                    <span class="right badge badge-danger"></span>
                                </p>
                            </a>
                        </li>
                        <li class="nav-item">
                            <a href="/HızlıBakış/HızlıBakış/" class="nav-link">
                                <i class="fa fa-sliders-h"></i>
                                <p>
                                    Hızlı Bakış
                                    <span class="right badge badge-danger"></span>
                                </p>
                            </a>
                        </li>
                        <li class="nav-item">
                            <a href="/Fatura/Liste/" class="nav-link">
                                <i class="far fa-file-alt"></i>
                                <p>
                                    Faturalar
                                    <span class="right badge badge-danger"></span>
                                </p>
                            </a>
                        </li>

                        @if (User.IsInRole("A"))
                        {
                            <li class="nav-item has-treeview">
                                <a href="#" class="nav-link">
                                    <i class="fa fa-file"></i>
                                    <p>
                                        Raporlar
                                        <i class="right fas fa-angle-left"></i>
                                    </p>
                                </a>
                                <ul class="nav nav-treeview">
                                    <li class="nav-item">
                                        <a href="/Ürünler/ÜrünRapor/" class="nav-link ">
                                            <i class="far fa-circle nav-icon"></i>
                                            <p>Ürün Raporları</p>
                                        </a>
                                    </li>
                                    <li class="nav-item">
                                        <a href="/Satışlar/SatışRapor/" class="nav-link">
                                            <i class="far fa-circle nav-icon"></i>
                                            <p>Satış Raporları</p>
                                        </a>
                                    </li>
                                    <li class="nav-item">
                                        <a href="/Personeller/PersonelRapor/" class="nav-link">
                                            <i class="far fa-circle nav-icon"></i>
                                            <p>Personel Raporları</p>
                                        </a>
                                    </li>
                                    <li class="nav-item">
                                        <a href="/Müşteriler/MüşteriRapor/" class="nav-link">
                                            <i class="far fa-circle nav-icon"></i>
                                            <p>Müşteri Raporları</p>
                                        </a>
                                    </li>
                                    <li class="nav-item">
                                        <a href="./index3.html" class="nav-link">
                                            <i class="far fa-circle nav-icon"></i>
                                            <p>Gider Raporları</p>
                                        </a>
                                    </li>
                                </ul>
                            </li>
                        }

                        <li class="nav-item has-treeview">
                            <a href="#" class="nav-link">
                                <i class="fa fa-chart-area"></i>
                                <p>
                                    Grafikler
                                    <i class="right fas fa-angle-left"></i>
                                </p>
                            </a>
                            <ul class="nav nav-treeview">
                                <li class="nav-item">
                                    <a href="/Grafikler/Index2/" class="nav-link ">
                                        <i class="far fa-circle nav-icon"></i>
                                        <p>Ürün-Stok Grafiği</p>
                                    </a>
                                </li>
                                <li class="nav-item">
                                    <a href="/Grafikler/Index/" class="nav-link ">
                                        <i class="far fa-circle nav-icon"></i>
                                        <p>Personel-Departman Grafiği</p>
                                    </a>
                                </li>
                                <li class="nav-item">
                                    <a href="/Grafikler/Index3/" class="nav-link ">
                                        <i class="far fa-circle nav-icon"></i>
                                        <p>Personel-Satış Grafiği</p>
                                    </a>
                                </li>
                                <li class="nav-item">
                                    <a href="/Grafikler/Index4/" class="nav-link ">
                                        <i class="far fa-circle nav-icon"></i>
                                        <p>Ürün-Satış Grafiği</p>
                                    </a>
                                </li>

                            </ul>
                        </li>
                        


                        <li class="nav-item">
                            <a href="/AdminLogin/LogOut/" class="nav-link">
                                <i class="fa fa-times"></i>
                                <p>
                                    Çıkış Yap
                                    <span class="right badge badge-danger"></span>
                                </p>
                            </a>
                        </li>






                    </ul>
                </nav>
                <!-- /.sidebar-menu -->
            </div>
            <!-- /.sidebar -->
        </aside>

        <!-- Content Wrapper. Contains page content -->
        <div class="content-wrapper">
            @RenderBody()
        </div>
        <!-- /.content-wrapper -->
        

        <!-- Control Sidebar -->
        <aside class="control-sidebar control-sidebar-dark">
            <!-- Control sidebar content goes here -->
        </aside>
        <!-- /.control-sidebar -->
    </div>
    <!-- ./wrapper -->
    <!-- jQuery -->
    <script src="/Klasörler/web4/plugins/jquery/jquery.min.js"></script>
    <!-- jQuery UI 1.11.4 -->
    <script src="/Klasörler/web4/plugins/jquery-ui/jquery-ui.min.js"></script>
    <!-- Resolve conflict in jQuery UI tooltip with Bootstrap tooltip -->
    <script>
        $.widget.bridge('uibutton', $.ui.button)
    </script>
    <!-- Bootstrap 4 -->
    <script src="/Klasörler/web4/plugins/bootstrap/js/bootstrap.bundle.min.js"></script>
    <!-- ChartJS -->
    <script src="/Klasörler/web4/plugins/chart.js/Chart.min.js"></script>
    <!-- Sparkline -->
    <script src="/Klasörler/web4/plugins/sparklines/sparkline.js"></script>
    <!-- JQVMap -->
    <script src="/Klasörler/web4/plugins/jqvmap/jquery.vmap.min.js"></script>
    <script src="/Klasörler/web4/plugins/jqvmap/maps/jquery.vmap.usa.js"></script>
    <!-- jQuery Knob Chart -->
    <script src="/Klasörler/web4/plugins/jquery-knob/jquery.knob.min.js"></script>
    <!-- daterangepicker -->
    <script src="/Klasörler/web4/plugins/moment/moment.min.js"></script>
    <script src="plugins/daterangepicker/daterangepicker.js"></script>
    <!-- Tempusdominus Bootstrap 4 -->
    <script src="/Klasörler/web4/plugins/tempusdominus-bootstrap-4/js/tempusdominus-bootstrap-4.min.js"></script>
    <!-- Summernote -->
    <script src="/Klasörler/web4/plugins/summernote/summernote-bs4.min.js"></script>
    <!-- overlayScrollbars -->
    <script src="/Klasörler/web4/plugins/overlayScrollbars/js/jquery.overlayScrollbars.min.js"></script>
    <!-- AdminLTE App -->
    <script src="/Klasörler/web4/dist/js/adminlte.js"></script>
    <!-- AdminLTE dashboard demo (This is only for demo purposes) -->
    <script src="/Klasörler/web4/dist/js/pages/dashboard.js"></script>
    <!-- AdminLTE for demo purposes -->
    <script src="/Klasörler/web4/dist/js/demo.js"></script>
</body>
</html>
