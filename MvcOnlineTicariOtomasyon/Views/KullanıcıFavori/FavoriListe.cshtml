                <section class="lattest-product-area pb-40 category-list">
                    <div class="row">
                        @foreach (var item in Model.fvr)
                        {


                        <!-- single product -->
                        <div class="col-lg-4 col-md-6">
                            <div class="single-product">
                                <img class="img-fluid" src="@item.Ürünler.ÜrünGörsel" style="width:350px;height:300px" alt="">
                                <div class="product-details">
                                    <h6>
                                       @item.Ürünler.ÜrünAd                                    </h6>
                                    <div class="price">
                                        <h6>@item.Ürünler.SatışFiyat</h6>
                                        <h6 class="l-through">$210.00</h6>
                                    </div>
                                    <div class="prd-bottom">

                                        <a href="/KullanıcıSepetim/SepeteEkle/@item.ÜrünId" class="social-info">
                                            <span class="ti-bag"></span>
                                            <p class="hover-text">Sepete Ekle</p>
                                        </a>
                                        <a href="/KullanıcıÜrünler/FavoriÇıkar/@item.ÜrünId" class="social-info">
                                            <span class="lnr lnr-heart"></span>
                                            <p class="hover-text">FAvori Sİl</p>
                                        </a>
                                     
                                        <a href="/KullanıcıÜrünler/Index" class="social-info">
                                            <span class="lnr lnr-move"></span>
                                            <p class="hover-text">Daha Fazla</p>
